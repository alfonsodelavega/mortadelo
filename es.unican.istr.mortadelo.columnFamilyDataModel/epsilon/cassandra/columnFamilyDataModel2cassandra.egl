[%
import "columnFamilyDataModel2cassandraHelpers.eol";

for (cf in columnFamilyDataModel!ColumnFamily){ %]
  CREATE TABLE [%=cf.name%] (
  [% for (co in cf.columns) { %]
    [%=co.name%] [%=co.getType()%],
  [% } %]
    PRIMARY KEY(([%=cf.getPartitionKeys()%])[%=cf.getClusteringKeys()%])
  );

[%
}
%]