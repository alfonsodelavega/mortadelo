<?xml version="1.0" encoding="ASCII"?>
<gdmLang:Model xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:gdmLang="http://www.unican.es/istr/mortadelo/gdm/lang/GdmLang">
  <entities name="Client">
    <features xsi:type="gdmLang:Attribute" name="clientId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="name" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="nationality" type="text"/>
    <features xsi:type="gdmLang:Reference" name="purchases" entity="//@entities.5" cardinality="*"/>
  </entities>
  <entities name="Seller">
    <features xsi:type="gdmLang:Attribute" name="sellerId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="name" type="text"/>
  </entities>
  <entities name="Address">
    <features xsi:type="gdmLang:Attribute" name="addressId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="street" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="postalCode" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="city" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="country" type="text"/>
  </entities>
  <entities name="Category">
    <features xsi:type="gdmLang:Attribute" name="categoryId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="name" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="description" type="text"/>
  </entities>
  <entities name="Product">
    <features xsi:type="gdmLang:Attribute" name="productId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="name" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="description" type="text"/>
    <features xsi:type="gdmLang:Attribute" name="price"/>
    <features xsi:type="gdmLang:Reference" name="categories" entity="//@entities.3" cardinality="*"/>
    <features xsi:type="gdmLang:Reference" name="seller" entity="//@entities.1" cardinality="1"/>
  </entities>
  <entities name="Purchase">
    <features xsi:type="gdmLang:Attribute" name="purchaseId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="purchaseDate" type="date"/>
    <features xsi:type="gdmLang:Reference" name="client" entity="//@entities.0" cardinality="1"/>
    <features xsi:type="gdmLang:Reference" name="shippingAddress" entity="//@entities.2" cardinality="1"/>
    <features xsi:type="gdmLang:Reference" name="bill" entity="//@entities.7" cardinality="1"/>
    <features xsi:type="gdmLang:Reference" name="lines" entity="//@entities.6" cardinality="*"/>
  </entities>
  <entities name="PurchaseLine">
    <features xsi:type="gdmLang:Attribute" name="lineNumber"/>
    <features xsi:type="gdmLang:Attribute" name="quantity"/>
    <features xsi:type="gdmLang:Attribute" name="unitPrice"/>
    <features xsi:type="gdmLang:Reference" name="product" entity="//@entities.4" cardinality="1"/>
  </entities>
  <entities name="Bill">
    <features xsi:type="gdmLang:Attribute" name="billId" type="id"/>
    <features xsi:type="gdmLang:Attribute" name="billDate" type="date"/>
    <features xsi:type="gdmLang:Attribute" name="billingData" type="text"/>
  </entities>
  <queries name="Q1_productById">
    <projections refAlias="//@queries.0/@from/@alias" attribute="//@entities.4/@features.0"/>
    <projections refAlias="//@queries.0/@from/@alias" attribute="//@entities.4/@features.1"/>
    <projections refAlias="//@queries.0/@from/@alias" attribute="//@entities.4/@features.2"/>
    <projections refAlias="//@queries.0/@from/@alias" attribute="//@entities.4/@features.3"/>
    <from entity="//@entities.4">
      <alias name="prod"/>
    </from>
    <condition xsi:type="gdmLang:Equality" value="?">
      <selection refAlias="//@queries.0/@from/@alias" attribute="//@entities.4/@features.0"/>
    </condition>
  </queries>
  <queries name="Q2_productByNameAndCategory">
    <projections refAlias="//@queries.1/@from/@alias" attribute="//@entities.4/@features.1"/>
    <projections refAlias="//@queries.1/@from/@alias" attribute="//@entities.4/@features.2"/>
    <projections refAlias="//@queries.1/@from/@alias" attribute="//@entities.4/@features.3"/>
    <projections refAlias="//@queries.1/@inclusions.0/@alias" attribute="//@entities.3/@features.1"/>
    <from entity="//@entities.4">
      <alias name="prod"/>
    </from>
    <inclusions refAlias="//@queries.1/@from/@alias" refs="//@entities.4/@features.4">
      <alias name="cat"/>
    </inclusions>
    <condition xsi:type="gdmLang:Equality" value="?">
      <selection refAlias="//@queries.1/@from/@alias" attribute="//@entities.4/@features.1"/>
    </condition>
  </queries>
  <queries name="Q3_productsByCategory">
    <projections refAlias="//@queries.2/@from/@alias" attribute="//@entities.4/@features.1"/>
    <projections refAlias="//@queries.2/@from/@alias" attribute="//@entities.4/@features.2"/>
    <projections refAlias="//@queries.2/@from/@alias" attribute="//@entities.4/@features.3"/>
    <projections refAlias="//@queries.2/@inclusions.0/@alias" attribute="//@entities.3/@features.1"/>
    <from entity="//@entities.4">
      <alias name="prod"/>
    </from>
    <inclusions refAlias="//@queries.2/@from/@alias" refs="//@entities.4/@features.4">
      <alias name="cat"/>
    </inclusions>
    <condition xsi:type="gdmLang:Equality" value="?">
      <selection refAlias="//@queries.2/@inclusions.0/@alias" attribute="//@entities.3/@features.1"/>
    </condition>
    <orderingAttributes refAlias="//@queries.2/@from/@alias" attribute="//@entities.4/@features.3"/>
  </queries>
  <queries name="Q4_purchasesWithBills">
    <projections refAlias="//@queries.3/@from/@alias" attribute="//@entities.5/@features.0"/>
    <projections refAlias="//@queries.3/@from/@alias" attribute="//@entities.5/@features.1"/>
    <projections refAlias="//@queries.3/@inclusions.1/@alias" attribute="//@entities.0/@features.1" alias="clientName"/>
    <projections refAlias="//@queries.3/@inclusions.0/@alias" attribute="//@entities.7/@features.1"/>
    <projections refAlias="//@queries.3/@inclusions.0/@alias" attribute="//@entities.7/@features.2"/>
    <from entity="//@entities.5">
      <alias name="pur"/>
    </from>
    <inclusions refAlias="//@queries.3/@from/@alias" refs="//@entities.5/@features.4">
      <alias name="bill"/>
    </inclusions>
    <inclusions refAlias="//@queries.3/@from/@alias" refs="//@entities.5/@features.2">
      <alias name="cl"/>
    </inclusions>
    <condition xsi:type="gdmLang:Equality" value="?">
      <selection refAlias="//@queries.3/@from/@alias" attribute="//@entities.5/@features.1" function="year"/>
    </condition>
    <orderingAttributes refAlias="//@queries.3/@from/@alias" attribute="//@entities.5/@features.1"/>
  </queries>
  <queries name="Q5_purchasesWithUserAndLines">
    <projections refAlias="//@queries.4/@from/@alias" attribute="//@entities.5/@features.0"/>
    <from entity="//@entities.5">
      <alias name="pur"/>
    </from>
    <inclusions refAlias="//@queries.4/@from/@alias" refs="//@entities.5/@features.5">
      <alias name="plines"/>
    </inclusions>
    <inclusions refAlias="//@queries.4/@from/@alias" refs="//@entities.5/@features.2">
      <alias name="cl"/>
    </inclusions>
    <condition xsi:type="gdmLang:Equality" value="?">
      <selection refAlias="//@queries.4/@inclusions.1/@alias" attribute="//@entities.0/@features.2"/>
    </condition>
    <orderingAttributes refAlias="//@queries.4/@from/@alias" attribute="//@entities.5/@features.1"/>
  </queries>
</gdmLang:Model>
